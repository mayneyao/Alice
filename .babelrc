{
    "presets": [
        "@babel/preset-env"
    ],
    "plugins": [
        [
            "@babel/plugin-transform-react-jsx",
            {
                "pragma": "Alice.createElement", // default pragma is React.createElement
                "pragmaFrag": "", // default is React.Fragment
                "throwIfNamespace": false // defaults to true
            }
        ]
    ]
}